local function toggleShiftLock()
    shiftLockEnabled = not shiftLockEnabled
    ToggleButton.Text = shiftLockEnabled and "Shift Lock On" or "Shift Lock Off"
end
ToggleButton.MouseButton1Click:Connect(toggleShiftLock)


SettingsButton.MouseButton1Click:Connect(function()
    SettingsPanel.Visible = not SettingsPanel.Visible
end)


IncreaseButton.MouseButton1Click:Connect(function()
    lerpSpeed = math.clamp(lerpSpeed + 0.5, 0.1, 20)
    SpeedLabel.Text = "Look Speed: " .. string.format("%.1f", lerpSpeed)
end)

DecreaseButton.MouseButton1Click:Connect(function()
    lerpSpeed = math.clamp(lerpSpeed - 0.5, 0.1, 20)
    SpeedLabel.Text = "Look Speed: " .. string.format("%.1f", lerpSpeed)
end)


local function makeMovable(frame)
    local dragging = false
    local dragStart, startPos

    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    frame.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.Touch then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(
                startPos.X.Scale,
                startPos.X.Offset + delta.X,
                startPos.Y.Scale,
                startPos.Y.Offset + delta.Y
            )
        end
    end)
end

makeMovable(ToggleButton)
makeMovable(SettingsButton)
makeMovable(SettingsPanel)



local function updateCameraOffset(deltaTime)
    lastUpdate = lastUpdate + deltaTime
    if lastUpdate < updateInterval then return end
    lastUpdate = 0

    local character = LocalPlayer.Character
    if not character then return end

    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local root = character:FindFirstChild("HumanoidRootPart")
    if not humanoid or not root then return end

    if shiftLockEnabled then
        humanoid.CameraOffset = Vector3.new(1.5, 0, 0)
        local camLook = Camera.CFrame.LookVector
        local targetCFrame = CFrame.new(root.Position) * CFrame.Angles(0, math.atan2(-camLook.X, -camLook.Z), 0)
        root.CFrame = root.CFrame:Lerp(targetCFrame, lerpSpeed)
    else
        humanoid.CameraOffset = Vector3.new(0, 0, 0)
    end
end

RunService.RenderStepped:Connect(updateCameraOffset)


UserInputService.InputBegan:Connect(function(input, processed)
    if not processed and input.KeyCode == Enum.KeyCode.LeftShift then
        toggleShiftLock()
    end
end)
    end
})

