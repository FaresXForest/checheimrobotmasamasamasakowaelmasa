-- âš¡ Ø³ÙƒØ±Ø¨Øª 3 - Advanced DESYNC
TabBrainrot:CreateButton({
    Name = "Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ø¯Ù‡ Ø¨ÙŠÙƒØ¨Ø± Ø³ÙƒÙ†Ùƒ Ù…Ù†Ø§Ø³Ø¨ Ù„Ù„ PVP ",
    Callback = function()
        
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local ReplicatedStorage = game:GetService("ReplicatedStorage")


local DesyncConfig = {
    Enabled = true,
    Mode = "Advanced", -- Basic, Advanced, Ghost, Predator
    Intensity = 8, -- Ù…Ù† 1 Ø¥Ù„Ù‰ 10
    
    
    SpeedMultiplier = 2.5,
    FakePosition = true,
    TeleportInterval = 15,
    
    HitboxDesync = true,
    HitboxSize = 3, -- ØªÙƒØ¨ÙŠØ± Ø§Ù„Ù‡ÙŠØªØ¨ÙˆÙƒØ³
    InvisibleHitbox = false,
    
    
    AnimationDesync = true,
    FakeAnimations = true,
    
    
    AntiTarget = true,
    RandomName = true,
    
    
    AntiKick = true,
    AutoAdjust = true
}


local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")


local originalWalkSpeed = humanoid.WalkSpeed
local originalJumpPower = humanoid.JumpPower
local originalHipHeight = humanoid.HipHeight


local function createBasicDesync()
    if not DesyncConfig.Enabled then return end
    
    humanoid.WalkSpeed = originalWalkSpeed * DesyncConfig.SpeedMultiplier
    
    spawn(function()
        while DesyncConfig.Enabled and DesyncConfig.FakePosition do
            wait(DesyncConfig.TeleportInterval)
            local fakePosition = rootPart.Position + Vector3.new(
                math.random(-10, 10),
                0,
                math.random(-10, 10)
            )
            humanoid:MoveTo(fakePosition)
        end
    end)
end


local function createHitboxDesync()
    if not DesyncConfig.Enabled or not DesyncConfig.HitboxDesync then return end
    
    local function modifyHitboxes()
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                part.Size = part.Size * DesyncConfig.HitboxSize
                if DesyncConfig.InvisibleHitbox then
                    part.Transparency = 0.8
                    part.Material = Enum.Material.ForceField
                end
            end
        end
    end
    
    character.ChildAdded:Connect(function(child)
        if child:IsA("BasePart") then
            wait(0.5)
            modifyHitboxes()
        end
    end)
    
    modifyHitboxes()
end


local function createAnimationDesync()
    if not DesyncConfig.Enabled or not DesyncConfig.AnimationDesync then return end
    
    humanoid:LoadAnimation(Instance.new("Animation")).Name = "FakeAnimation"
    
    spawn(function()
        while DesyncConfig.Enabled do
            humanoid.AutoRotate = math.random() > 0.5
            if math.random(1, 10) > 7 then
                humanoid.Jump = true
            end
            wait(0.5)
        end
    end)
end


local function createAntiTargetSystem()
    if not DesyncConfig.Enabled or not DesyncConfig.AntiTarget then return end
    
    spawn(function()
        while DesyncConfig.Enabled do
            if DesyncConfig.RandomName then
                player.Character:FindFirstChild("Humanoid").DisplayName = "Player" .. math.random(1000, 9999)
            end
            
            local zigzag = math.sin(tick() * 10) * 5
            humanoid:Move(Vector3.new(zigzag, 0, 1))
            wait(0.1)
        end
    end)
end


local function createAdvancedPositionDesync()
    if not DesyncConfig.Enabled then return end
    
    local lastRealPosition = rootPart.Position
    local desyncOffset = Vector3.new(0, 0, 0)
    
    spawn(function()
        while DesyncConfig.Enabled do
            desyncOffset = Vector3.new(
                math.sin(tick()) * 5,
                math.cos(tick() * 0.5) * 2,
                math.sin(tick() * 1.5) * 5
            )
            if rootPart then
                rootPart.CFrame = CFrame.new(lastRealPosition + desyncOffset)
            end
            wait(0.1)
        end
    end)
    
    spawn(function()
        while DesyncConfig.Enabled do
            lastRealPosition = rootPart.Position - desyncOffset
            wait(0.2)
        end
    end)
end


local function createFakeLagSystem()
    spawn(function()
        while DesyncConfig.Enabled do
            wait(math.random(10, 30))
            humanoid.WalkSpeed = 0
            wait(0.5)
            humanoid.WalkSpeed = originalWalkSpeed * DesyncConfig.SpeedMultiplier
            
        end
    end)
end


local function createAutoAdjustSystem()
    if not DesyncConfig.Enabled or not DesyncConfig.AutoAdjust then return end
    
    spawn(function()
        while DesyncConfig.Enabled do
            local nearbyPlayers = 0
            for _, otherPlayer in pairs(Players:GetPlayers()) do
                if otherPlayer ~= player and otherPlayer.Character then
                    local distance = (otherPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
                    if distance < 50 then
                        nearbyPlayers = nearbyPlayers + 1
                    end
                end
            end
            if nearbyPlayers > 3 then
                DesyncConfig.Intensity = math.min(5, DesyncConfig.Intensity)
            else
                DesyncConfig.Intensity = 8
            end
            wait(5)
        end
    end)
end


local function createControlPanel()
    
    
    
    
    
    
    spawn(function()
        while DesyncConfig.Enabled do
            wait(1)
            local status = "ğŸŸ¢ Ù†Ø´Ø·"
            
        end
    end)
end


local function initializeDesyncSystem()
    if not DesyncConfig.Enabled then return end
    
    if not character or not rootPart then
        player.CharacterAdded:Wait()
        character = player.Character
        rootPart = character:WaitForChild("HumanoidRootPart")
        humanoid = character:WaitForChild("Humanoid")
    end
    
    createBasicDesync()
    createHitboxDesync()
    createAnimationDesync()
    createAntiTargetSystem()
    createAdvancedPositionDesync()
    createFakeLagSystem()
    createAutoAdjustSystem()
    createControlPanel()
    
    
    
    
end


player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    humanoid = character:WaitForChild("Humanoid")
    rootPart = character:WaitForChild("HumanoidRootPart")
    wait(2) 
    initializeDesyncSystem()
end)


initializeDesyncSystem()


return {
    Config = DesyncConfig,
    
    setIntensity = function(intensity)
        DesyncConfig.Intensity = math.clamp(intensity, 1, 10)
        humanoid.WalkSpeed = originalWalkSpeed * (1 + DesyncConfig.Intensity * 0.3)
    end,
    
    toggleDesync = function()
        DesyncConfig.Enabled = not DesyncConfig.Enabled
        if DesyncConfig.Enabled then
            initializeDesyncSystem()
        else
            humanoid.WalkSpeed = originalWalkSpeed
            
        end
    end,
    
    emergencyStop = function()
        DesyncConfig.Enabled = false
        humanoid.WalkSpeed = originalWalkSpeed
        humanoid.JumpPower = originalJumpPower
        
    end
}
        
    end
})

