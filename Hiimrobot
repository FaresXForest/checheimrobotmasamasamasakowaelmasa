WarningText.AutomaticSize = Enum.AutomaticSize.Y WarningText.Position = UDim2.new(0.05, 0, 0, 35); WarningText.BackgroundTransparency = 1; WarningText.Text ="  Ø´ÙƒØ±Ø§ Ù„ÙŠÙƒ Ù„ØªØ´ØºÙŠÙ„ Ø³ÙƒØ±Ø¨Øª aimbot Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ø¯Ù„ÙˆÙ‚ØªÙŠ Ù‡ÙŠØ´ØªØºÙ„ Ù…Ø¹Ø§Ùƒ ÙˆÙ„ÙƒÙ† Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ø¯Ù‡ Ø¨Ù†Ø³Ø¨Ù‡ 80 ÙÙŠ Ø§Ù„Ù…ÙŠÙ‡ Ù‡ØªÙ‚ÙˆÙ„ Ù„ÙŠÙ‡ Ø¯Ù‡ Ø§ÙŠÙ…Ø¨ÙˆØª Ø§Ù„Ù…ÙØ±ÙˆØ¶ 100 ÙÙŠ Ø§Ù„Ù…ÙŠÙ‡ Ù‡Ùˆ ÙØ¹Ù„Ø§ 100 ÙÙŠ Ø§Ù„Ù…ÙŠÙ‡ ÙˆÙ„ÙƒÙ† Ù‡Ùˆ 80 Ø¨Ø³Ø¨Ø¨ Ø§Ù„ÙƒØ§Ù…ÙŠØ±Ø§ Ùˆ Ø§Ù†Øª Ø¹Ø§Ø±Ù Ø§Ù† Ù…Ø§Ø¨ stealabrainrot Ø¨ØªÙ…Ù†Ø¹ ØªØ­Ø±ÙŠÙƒ Ø´Ø®ØµÙŠÙ‡ Ø¹Ù„ÙŠ Ø§Ù„Ø®ØµÙ… ÙØ§ Ø¯Ù„ÙˆÙ‚ØªÙŠÙ‡ØªØ­ØªØ§Ø¬ ØªØ´ØºÙ„ Ø³ÙƒØ±Ø¨ØªÙŠÙ† Ù…Ù† ØµÙ†Ø¹ÙŠ Ø³ÙƒØ±Ø¨Øª SL Ù…ØªÙ‚Ù„Ù‚Ø´ Ø¯ÙŠ Ù†Ø³Ø®Ù‡ Ø§Ù„ v6 Ù…Ø´ Ù‡ØªØ­Ø±ÙƒÙƒ ØªÙ„Ù‚Ø§Ø¦ÙŠ Ø¯ÙŠ Ø¨Ø³ Ù‡ØªØ«Ø¨Øª Ø´Ø®ØµØªÙŠÙƒ Ø¹Ø´Ø§Ù† Ø§Ù„Ø®Ø·ÙˆÙ‡ Ø§Ù„ØªØ§Ù†ÙŠÙ‡ ØªÙ†Ø¬Ø­ Ø§Ù„Ø®Ø·ÙˆÙ‡ Ø§Ù„ØªØ§Ù†ÙŠÙ‡ Ù‡ØªØ´ØºÙ„ Ø³ÙƒØ±Ø¨Øª shift lock Ø§Ù„ÙŠ Ù…Ù† ØµÙ†Ø¹ÙŠ Ùˆ Ù‡ØªÙØªØ­ settings Ùˆ ØªØ®Ù„ÙŠ Ø±Ù‚Ù… Ø¹Ù„ÙŠ 1.2 Ùˆ ØªØ¹Ù…Ù„ Ù„Ù„ Ø´ÙŠÙØª on Ù„Ùˆ Ø·Ø¨Ù‚Øª ÙƒÙ„ Ø¯Ù‡ Ø§Ø­Ø¨ Ø§Ø·Ù…Ù†Ùƒ Ø§Ù†Ùƒ ÙƒØ¯Ø§ Ø¨ØªØ³ØªØ¹Ù…Ù„ aimbot Ø¨Ù†Ø³Ø¨Ù‡ 100 ÙÙŠ Ø§Ù„Ù…ÙŠÙ‡."; WarningText.TextColor3 = Color3.fromRGB(255, 200, 200); WarningText.Font = Enum.Font.SourceSans; WarningText.TextSize = 14; WarningText.TextWrapped = true; WarningText.Parent = WarningFrame; local CloseWarningButton = Instance.new("TextButton"); CloseWarningButton.Size = UDim2.new(0, 100, 0, 25); CloseWarningButton.Position = UDim2.new(0.5, -50, 1, -30); CloseWarningButton.BackgroundColor3 = Color3.fromRGB(80, 80, 80); CloseWarningButton.Text = "ok"; CloseWarningButton.TextColor3 = Color3.fromRGB(255, 255, 255); CloseWarningButton.Font = Enum.Font.SourceSansBold; CloseWarningButton.Parent = WarningFrame


local function MakeDraggable(guiObject)
    local dragging = false
    local dragInput = nil
    local dragStart = nil
    local startPos = nil

    guiObject.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragInput = input
            dragStart = input.Position
            startPos = guiObject.Position
        end
    end)

    guiObject.InputEnded:Connect(function(input)
        if input == dragInput then
            dragging = false
            dragInput = nil
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            guiObject.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
end


function UpdatePlayerList() for i, v in pairs(ScrollingFrame:GetChildren()) do if v:IsA("TextButton") then v:Destroy() end end; local players = Players:GetPlayers(); ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, #players * 35); for i, player in pairs(players) do if player ~= LocalPlayer then local PlayerButton = Instance.new("TextButton"); PlayerButton.Name = player.Name; PlayerButton.Size = UDim2.new(1, -10, 0, 30); PlayerButton.BackgroundColor3 = Color3.fromRGB(50, 50, 60); PlayerButton.Text = player.Name; PlayerButton.TextColor3 = Color3.fromRGB(220, 220, 220); PlayerButton.Font = Enum.Font.SourceSans; PlayerButton.TextSize = 14; PlayerButton.Parent = ScrollingFrame; PlayerButton.MouseButton1Click:Connect(function() TargetPlayer = player; TargetInfoLabel.Text = "Target: " .. player.Name; TargetInfoLabel.TextColor3 = Color3.fromRGB(100, 255, 100) end) end end end
function CreateBodyPartButtons() local parts = {"Head", "Torso", "HumanoidRootPart"}; for _, partName in pairs(parts) do local button = Instance.new("TextButton"); button.Name = partName; button.BackgroundColor3 = Color3.fromRGB(60, 60, 70); button.Text = partName; button.TextColor3 = Color3.fromRGB(255, 255, 255); button.Font = Enum.Font.SourceSans; button.TextSize = 12; button.Parent = BodyPartFrame; BodyPartButtons[partName] = button; button.MouseButton1Click:Connect(function() TargetPartName = partName; for name, btn in pairs(BodyPartButtons) do btn.BackgroundColor3 = (name == partName) and Color3.fromRGB(85, 85, 125) or Color3.fromRGB(60, 60, 70) end end) end; if BodyPartButtons[DEFAULT_TARGET_PART] then BodyPartButtons[DEFAULT_TARGET_PART].BackgroundColor3 = Color3.fromRGB(85, 85, 125) end end
function GetTargetPart(character, partName) if partName == "Torso" then return character:FindFirstChild("UpperTorso") or character:FindFirstChild("Torso") else return character:FindFirstChild(partName) end end
function AimbotLoop() if AimbotEnabled and IsAiming and TargetPlayer and TargetPlayer.Character then local myRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart"); local targetPart = GetTargetPart(TargetPlayer.Character, TargetPartName); if myRoot and targetPart then if Camera.CameraType ~= Enum.CameraType.Scriptable then Camera.CameraType = Enum.CameraType.Scriptable end; local aimAtPosition = targetPart.CFrame.Position + Vector3.new(0, CAMERA_VERTICAL_OFFSET, 0); local cameraPivot = myRoot.CFrame * CFrame.new(CAMERA_SIDE_OFFSET, CAMERA_VERTICAL_OFFSET, 0); local cameraPosition = cameraPivot.Position - ((aimAtPosition - cameraPivot.Position).Unit * CAMERA_DISTANCE_BEHIND); local newCameraCFrame = CFrame.new(cameraPosition, aimAtPosition); Camera.CFrame = newCameraCFrame end else if Camera.CameraType == Enum.CameraType.Scriptable then Camera.CameraType = OriginalCameraType end end end



local isDragging = false
local dragThreshold = 5 
ToggleGUIButton.InputBegan:Connect(function(input) isDragging = false end)
ToggleGUIButton.InputChanged:Connect(function(input) if (input.Position - input.StartPosition).Magnitude > dragThreshold then isDragging = true end end)
ToggleGUIButton.InputEnded:Connect(function(input) if not isDragging then MainFrame.Visible = not MainFrame.Visible end end)

AimButton.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then IsAiming = true; AimButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50) end end)
AimButton.InputEnded:Connect(function(input) if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then IsAiming = false; AimButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50) end end)

ToggleAimbotButton.MouseButton1Click:Connect(function() AimbotEnabled = not AimbotEnabled; if AimbotEnabled then ToggleAimbotButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50); ToggleAimbotButton.Text = "Aimbot: ON" else ToggleAimbotButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50); ToggleAimbotButton.Text = "Aimbot: OFF" end end)
CloseWarningButton.MouseButton1Click:Connect(function() WarningFrame:Destroy() end)
Players.PlayerAdded:Connect(UpdatePlayerList)
Players.PlayerRemoving:Connect(UpdatePlayerList)
RunService.RenderStepped:Connect(AimbotLoop)


MakeDraggable(ToggleGUIButton)
MakeDraggable(AimButton)

UpdatePlayerList()
CreateBodyPartButtons()


    end
})

