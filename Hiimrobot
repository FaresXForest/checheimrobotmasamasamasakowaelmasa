            settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
            game:GetService("Lighting").GlobalShadows = false
            game:GetService("Lighting").FogEnd = 999999
            game:GetService("Lighting").Brightness = 2
        end)
    end
})


Tab:CreateButton({
    Name = "ØªÙ‚Ù„ÙŠÙ„ Ø§Ù„Ø³Ø®ÙˆÙ†Ù‡ v2ğŸ”¥â„ï¸",
    Callback = function()
        local screen = Instance.new("ScreenGui", game:GetService("CoreGui"))
        local label = Instance.new("TextLabel", screen)
        label.Text = "ÙƒÙ„ Ø­Ø§Ø¬Ù‡ Ø´ØºØ§Ù„Ù‡ ÙˆØ§Ø³ØªÙ…ØªØ¹ â€” Ù‡ØªØµÙ…Ù„ ÙÙˆÙ‚ 600 Ø³Ø§Ø¹Ù‡ ğŸ”¥"
        label.Size = UDim2.new(1, 0, 0, 40)
        label.Position = UDim2.new(0, 0, 0, 0)
        label.BackgroundTransparency = 1
        label.TextColor3 = Color3.fromRGB(0, 200, 255)
        label.TextScaled = true

        task.spawn(function()
            while task.wait(10) do
                if game:GetService("RunService").IsRunning then
                    setfpscap(25 + math.random(0,5))
                    game:GetService("Lighting").Ambient = Color3.new(0, 0, 0)
                    game:GetService("Lighting").OutdoorAmbient = Color3.new(0, 0, 0)
                end
            end
        end)
    end
})
