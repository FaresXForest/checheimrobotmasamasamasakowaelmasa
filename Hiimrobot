-- GhostPreserver v10 Lite | فارس & FO Team Edition | Roblox Anti-Disconnect System
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local VirtualUser = game:GetService("VirtualUser")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local player = Players.LocalPlayer

-- GUI واجهة تحكم
local ScreenGui = Instance.new("ScreenGui", player.PlayerGui)
ScreenGui.ResetOnSpawn = false

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 250, 0, 120)
Frame.Position = UDim2.new(0.7, 0, 0.75, 0)
Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Frame.BackgroundTransparency = 0.3
Frame.Active = true
Frame.Draggable = true
Frame.BorderSizePixel = 2

local Button = Instance.new("TextButton", Frame)
Button.Size = UDim2.new(1, 0, 0.5, 0)
Button.Position = UDim2.new(0, 0, 0, 0)
Button.Text = "GhostPreserver: OFF"
Button.TextColor3 = Color3.new(1, 1, 1)
Button.BackgroundTransparency = 0.4

local Status = Instance.new("TextLabel", Frame)
Status.Size = UDim2.new(1, 0, 0.5, 0)
Status.Position = UDim2.new(0, 0, 0.5, 0)
Status.Text = "Status: Inactive"
Status.TextColor3 = Color3.new(1, 0, 0)
Status.BackgroundTransparency = 1
Status.Font = Enum.Font.SourceSansBold
Status.TextScaled = true

-- Rainbow Text Effect (خفيف)
local function RainbowEffect(textLabel)
	task.spawn(function()
		while true do
			for i = 0, 1, 0.01 do
				textLabel.TextColor3 = Color3.fromHSV(i, 1, 1)
				task.wait(0.1)  -- تخفيف الضغط
			end
		end
	end)
end

RainbowEffect(Status)

-- الوظائف الأساسية
local GhostPreserverEnabled = false
local LastHeartbeat = tick()

-- Remote Event حماية Heartbeat
local function FireHeartbeat()
	local remote = ReplicatedStorage:FindFirstChildOfClass("RemoteEvent")
	if remote then
		remote:FireServer("Heartbeat")
	end
end

-- Loop حماية (Optimized)
RunService.RenderStepped:Connect(function()
	if GhostPreserverEnabled then
		if tick() - LastHeartbeat >= 5 then  -- كل 5 ثواني فقط
			FireHeartbeat()
			player:SetAttribute("ActiveSim", math.random())
			LastHeartbeat = tick()
		end
	end
end)

-- حماية من AFK Timeout (Roblox Idle)
Players.LocalPlayer.Idled:Connect(function()
	if GhostPreserverEnabled then
		VirtualUser:CaptureController()
		VirtualUser:ClickButton2(Vector2.new(0,0))
	end
end)

-- زرار تشغيل/إيقاف النظام
Button.MouseButton1Click:Connect(function()
	GhostPreserverEnabled = not GhostPreserverEnabled
	Button.Text = GhostPreserverEnabled and "GhostPreserver: ON" or "GhostPreserver: OFF"
	Status.Text = GhostPreserverEnabled and "Status: Active - فارس & FO" or "Status: Inactive"
end)

print("GhostPreserver v10 Lite by فارس & FO Team | Loaded Successfully") end})
Tab:CreateButton({Name = "يقلل الفريمات و يحسن الهاتف", Callback = function()
--[[
    Data Saver Hardcore v5 - Ultimate Hook Edition
    Developed by FO FARES & TEAM FO
    اصمل علي ضمانتي
--]]

local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local SoundService = game:GetService("SoundService")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local LocalPlayer = Players.LocalPlayer

-- Disable Visual Effects
local function disableVisuals()
    Lighting.GlobalShadows = false
    Lighting.FogEnd = 1000000
    Lighting.Brightness = 0

    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("Decal") or v:IsA("Texture") then
            v:Destroy()
        elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v:Destroy()
        
        end
    end

end

-- Hook Network Calls for Packet Throttling and Blocking
local mt = getrawmetatable(game)
setreadonly(mt, false)
local oldNamecall = mt.__namecall
local lastPacketSent = 0

mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    if method == "FireServer" or method == "InvokeServer" then
        if tick() - lastPacketSent < 5 then
            return -- Block packet if within throttle window (5 seconds)
        end
        lastPacketSent = tick()
    end
    return oldNamecall(self, ...)
end)

-- Disable Replication of New Instances
workspace.DescendantAdded:Connect(function(obj)
    if obj:IsA("Part") or obj:IsA("MeshPart") or obj:IsA("UnionOperation") then
        obj:Destroy()
    end
end)

-- Disable Character Animations Replication
RunService.Stepped:Connect(function()
    if LocalPlayer.Character then
        for _, anim in pairs(LocalPlayer.Character:GetDescendants()) do
            if anim:IsA("Animator") then
                anim:Destroy()
            end
        end
    end
end)

-- Physics Throttling (reducing physics update rate drastically)
settings().Physics.ThrottleAdjustTime = 10 -- Default is 0.5
settings().Physics.AllowSleep = true

-- Continuous Visual Cleanup
RunService.Heartbeat:Connect(function()
    disableVisuals()
end)

print("Data Saver Hardcore v5 Loaded: Extreme Network Throttling Active. Replication Blocked. Visuals Disabled.") end})
