-- Ø³ÙƒØ±Ø¨Øª Ø¨Ù†Ù‚ 20 (9)
Tab:CreateButton({
    Name = "Ping 20",
    Callback = function()
        local PerformanceStats = game:GetService("CoreGui"):WaitForChild("RobloxGui"):WaitForChild("PerformanceStats")
        local PingLabel
        for _, Child in next, PerformanceStats:GetChildren() do
            if Child:FindFirstChild("StatsMiniTextPanelClass") then
                local Title = Child.StatsMiniTextPanelClass:FindFirstChild("TitleLabel")
                if Title and Title.Text == "Ping" then
                    PingLabel = Child.StatsMiniTextPanelClass:FindFirstChild("ValueLabel")
                    break
                end
            end
        end
        local fakePing = "20.6 ms"
        if PingLabel then
            PingLabel:GetPropertyChangedSignal("Text"):Connect(function()
                PingLabel.Text = fakePing
            end)
            PingLabel.Text = fakePing
        end
