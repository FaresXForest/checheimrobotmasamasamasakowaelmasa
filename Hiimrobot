local dodging = false
local dodgeSpeed = 0.07
local dodgeDistance = 1.2

local function dodgeMove()
    local direction = 1
    while dodging do
        if not HumanoidRootPart.Parent then break end
        local currentPos = HumanoidRootPart.Position
        local offset = HumanoidRootPart.CFrame.RightVector * dodgeDistance * direction
        HumanoidRootPart.CFrame = CFrame.new(currentPos + offset)
        direction = -direction
        wait(dodgeSpeed)
    end
end


StartButton.MouseButton1Click:Connect(function()
    if not dodging then
        dodging = true
        spawn(dodgeMove)
    end
end)

StopButton.MouseButton1Click:Connect(function()
    dodging = false
end)


