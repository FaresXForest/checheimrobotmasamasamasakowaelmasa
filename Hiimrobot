TabBrainrot:CreateButton({
    Name = "Ø³ÙƒØ±Ø¨Øª SL Ù„Ø§Ø²Ù… ØªØ´ØºÙ„Ù‡ Ù…Ø¹ Ø§Ù„Ø§ÙŠÙ…Ø¨ÙˆØª Ùˆ Ø´ÙŠÙØª Ù„ÙˆÙƒ",
    Callback = function()
        
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local ContextActionService = game:GetService("ContextActionService")
local LocalPlayer = Players.LocalPlayer
local Camera = game.Workspace.CurrentCamera


local SHIFT_LOCK_OFFSET = Vector3.new(1.7, 0.5, 0)
local KEYBIND = Enum.KeyCode.LeftShift
local ACTION_NAME = "FaresMoveDirectionShiftLock_v6"


local isShiftLockActive = false
local isTouchDevice = UserInputService.TouchEnabled
local character, humanoid



local function updateCharacterRotation()
    if not isShiftLockActive or not humanoid or humanoid.Sit then return end
    


    local cameraLookVector = Camera.CFrame.LookVector
    local groundLookVector = Vector3.new(cameraLookVector.X, 0, cameraLookVector.Z).Unit
    


    humanoid.MoveDirection = groundLookVector
end


local function toggleShiftLock(actionName, inputState, inputObject)
    if inputState ~= Enum.UserInputState.Begin then return end
    
    isShiftLockActive = not isShiftLockActive
    
    UserInputService.MouseBehavior = isShiftLockActive and Enum.MouseBehavior.LockCenter or Enum.MouseBehavior.Default
    
    if humanoid then
    


        humanoid.AutoRotate = not isShiftLockActive
        humanoid.CameraOffset = isShiftLockActive and SHIFT_LOCK_OFFSET or Vector3.new(0, 0, 0)
    end
    
    if isTouchDevice then
        ContextActionService:SetTitle(ACTION_NAME, isShiftLockActive and "SL: ON" or "SL: OFF")
    end
end



RunService.RenderStepped:Connect(updateCharacterRotation)



local function onCharacterAdded(char)
    character = char
    humanoid = char:WaitForChild("Humanoid")
    
    
    
    if humanoid then
        humanoid.AutoRotate = not isShiftLockActive
        humanoid.CameraOffset = isShiftLockActive and SHIFT_LOCK_OFFSET or Vector3.new(0, 0, 0)
    end
end



if isTouchDevice then
    ContextActionService:BindAction(ACTION_NAME, toggleShiftLock, true)
    ContextActionService:SetPosition(ACTION_NAME, UDim2.new(1, -80, 0, 15))
    ContextActionService:SetTitle(ACTION_NAME, "SL: OFF")
else
    UserInputService.InputBegan:Connect(function(input, isProcessed)
        if not isProcessed and input.KeyCode == KEYBIND then
            toggleShiftLock(ACTION_NAME, Enum.UserInputState.Begin, input)
        end
    end)
end



LocalPlayer.CharacterAdded:Connect(onCharacterAdded)



if LocalPlayer.Character then
    onCharacterAdded(LocalPlayer.Character)
end

    end
})

