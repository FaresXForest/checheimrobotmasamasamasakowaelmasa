-- Ø³ÙƒØ±Ø¨Øª 2 - Ø§ÙˆØªÙˆ ÙƒÙ„ÙƒØ± Ø¨ÙŠÙÙŠØ¯Ùƒ ÙÙŠ ØµÙ…Ù„Ù‡
TabTools:CreateButton({
    Name = "Ø§ÙˆØªÙˆ ÙƒÙ„ÙƒØ± Ø¨ÙŠÙÙŠØ¯Ùƒ ÙÙŠ ØµÙ…Ù„Ù‡",
    Callback = function()
        local Players = game:GetService("Players")
        local player = Players.LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")

        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "AutoClickerGUI"
        screenGui.Parent = playerGui
        screenGui.ResetOnSpawn = false

        local mainFrame = Instance.new("Frame")
        mainFrame.Name = "MainFrame"
        mainFrame.Parent = screenGui
        mainFrame.Size = UDim2.new(0, 200, 0, 100)
        mainFrame.Position = UDim2.new(0.8, -200, 0.8, -100)
        mainFrame.BackgroundColor3 = Color3.fromRGB(255, 182, 193)
        mainFrame.BorderSizePixel = 0

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 15)
        corner.Parent = mainFrame

        local toggleButton = Instance.new("TextButton")
        toggleButton.Parent = mainFrame
        toggleButton.Size = UDim2.new(0.8, 0, 0.6, 0)
        toggleButton.Position = UDim2.new(0.1, 0, 0.2, 0)
        toggleButton.BackgroundColor3 = Color3.fromRGB(255, 105, 180)
        toggleButton.Text = "Ø´ØºÙ„Ùˆ ÙŠØ§ Ø§Ù„ÙˆØ§Ù†ğŸ˜"
        toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        toggleButton.TextScaled = true
        toggleButton.Font = Enum.Font.Cartoon

        local buttonCorner = Instance.new("UICorner")
        buttonCorner.CornerRadius = UDim.new(0, 10)
        buttonCorner.Parent = toggleButton

        local isClicking = false
        local clickConnection

        local function startClicking()
            if isClicking then return end
            isClicking = true
            toggleButton.Text = "ÙˆÙ‚ÙÙˆ ÙŠØ§ Ø²ÙˆØ¬ØªÙŠğŸ˜ğŸ³ï¸â€ğŸŒˆ ğŸ˜˜"
            toggleButton.BackgroundColor3 = Color3.fromRGB(255, 20, 147)
            clickConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if isClicking then
                    local vim = game:GetService("VirtualInputManager")
                    vim:SendMouseButtonEvent(workspace.CurrentCamera.ViewportSize.X / 2, workspace.CurrentCamera.ViewportSize.Y / 2, 0, true, game, 0)
                    wait(0.1)
                    vim:SendMouseButtonEvent(workspace.CurrentCamera.ViewportSize.X / 2, workspace.CurrentCamera.ViewportSize.Y / 2, 0, false, game, 0)
                end
                wait(1)
            end)
        end

        local function stopClicking()
            isClicking = false
            toggleButton.Text = "Ø´ØºÙ„ ÙŠØ§ Ø²ÙˆØ¬ØªÙŠğŸ³ï¸â€ğŸŒˆ"
            toggleButton.BackgroundColor3 = Color3.fromRGB(255, 105, 180)
            if clickConnection then
                clickConnection:Disconnect()
            end
        end

        toggleButton.MouseButton1Click:Connect(function()
            if isClicking then
                stopClicking()
            else
                startClicking()
            end
        end)
    end
})

