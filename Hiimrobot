-- AntiAFK
local afkEnabled = false
local afkConnection
local function toggleAFK(state)
	if state and not afkEnabled then
		afkConnection = Players.LocalPlayer.Idled:Connect(function()
			VirtualUser:Button1Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
		end)
		afkEnabled = true
	else
		if afkConnection then afkConnection:Disconnect() end
		afkEnabled = false
	end
end


local function autoClean()
	while true do
		game:FindService("Lighting").FogEnd = math.huge
		game:FindService("Lighting").GlobalShadows = false
		collectgarbage("collect")
		wait(60)
	end
end
task.spawn(autoClean)


local startTime = tick()
local timer = Instance.new("TextLabel", main)
timer.Position = UDim2.new(0, 0, 0.12, 0)
timer.Size = UDim2.new(1, 0, 0.1, 0)
timer.BackgroundTransparency = 1
timer.Font = Enum.Font.Code
timer.TextColor3 = Color3.new(0, 1, 0)
timer.TextScaled = true
task.spawn(function()
	while true do
		local elapsed = tick() - startTime
		local hours = math.floor(elapsed / 3600)
		local minutes = math.floor((elapsed % 3600) / 60)
		timer.Text = "Ù…Ø¯Ø© ØµÙ…Ù„Ù‡: " .. hours .. " Ø³Ø§Ø¹Ø© Ùˆ " .. minutes .. " Ø¯Ù‚ÙŠÙ‚Ø©"
		wait(60)
	end
end)


